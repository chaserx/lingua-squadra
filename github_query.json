query($login:String!) {
  organization(login: $login) {
    membersWithRole(first: 100) {
      edges {
        node {
          name
          avatarUrl
          repositories(first:100, privacy:PUBLIC) {
            edges {
              node {
                name
                primaryLanguage {
                  name
                }
              }
            }
          }
        }
      }
    }
  }
}
